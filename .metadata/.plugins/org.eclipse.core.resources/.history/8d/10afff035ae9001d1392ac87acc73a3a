	<table border="1">
		<tr>
				<%
				
				for (int i = 2; i < 10; i++) {
					out.println("<tr>");
					out.println("<th>");
					out.println(i + "단");
					out.println("</th>");
					out.println("<tr>");
				}
				%>
		</tr>
		<%
		for (int i = 2; i < 10; i++) {
			for (int j = 1; j < 10; j++) {
				out.println("<tr>");
				out.println("<td>" + i + "X" + j + "=" + i * j + "</td>");
				out.println("</tr>");
			}
		}
		%>
	</table>
